{
    "tabWidth": 4,
    "overrides": [
    {
      "files": ["*.html", "*.handlebars", "*.ejs"],
      "options": {
        "tabWidth": 2
      }
    }
    ],
    "semi": false,
    "quoteProps": "consistent",
    "singleQuote": true,
    "jsxSingleQuote":false,
    "bracketSameLine": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "insertPragma": true
}
